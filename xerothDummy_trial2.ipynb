    "        print(f\"Best parameters for ARIMA model: Order {best_order}\")\n",
    "\n",
    "        # Fit ARIMA model with best parameters\n",
    "        arima_model = ARIMA(supplier_data_corrected, order=best_order)\n",
    "        arima_model_fit = arima_model.fit()\n",
